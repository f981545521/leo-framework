<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.acyou.leo.tool.mapper.DataAnalysisMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.acyou.leo.tool.entity.DataAnalysis">
        <id column="id" property="id" />
        <result column="code" property="code" />
        <result column="day_time" property="dayTime" />
        <result column="content" property="content" />
        <result column="content_new" property="contentNew" />
        <result column="field1" property="field1" />
        <result column="field2" property="field2" />
        <result column="field3" property="field3" />
        <result column="platform" property="platform" />
        <result column="ext" property="ext" />
        <result column="del_flag" property="delFlag" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="remark" property="remark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, code, day_time, content, content_new, field1, field2, field3, platform, ext, del_flag, create_time, update_time, remark
    </sql>

</mapper>
